<h1>Listing organizations</h1>

<table>
  <tr>
  </tr>

<% for organization in @organizations %>
  <tr>
    <td><%= link_to 'Show', orgadmin_organization_path(organization) %></td>
    <td><%= link_to 'Edit', edit_orgadmin_organization_path(organization) %></td>
    <td><%= link_to 'Destroy', orgadmin_organization_path(organization), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

