<h1>Editing Organization</h1>

<%= error_messages_for :organization %>

<% form_for(@organization, :url => orgadmin_organization_path( @organization), :method => :put ) do |f| %>
 <ul>
  <%= render :partial => 'fields', :locals => {:f => f } -%>
  <li>
    <%= f.submit "Update" %>
  </li>
 </ul>
<% end %>

<%= link_to 'Show', orgadmin_organization_path(@organization) %>
| <%= link_to 'Back', orgadmin_organization_path(@organization) %>
