<h1>Listing style_infos</h1>


<table border="1">
  <tr>
  <% %w{color_primary color_secondary color_third color_standfor_1 color_standfor_2 color_header_1 color_header_2 color_button_1 color_button_2 color_fill_1 color_fill_2 }.each do |field| %>
   <td><%= field -%></td>
  <% end %>
  </tr>

<% for style_info in @style_infos %>
  <tr>
  <% %w{color_primary color_secondary color_third color_standfor_1 color_standfor_2 color_header_1 color_header_2 color_button_1 color_button_2 color_fill_1 color_fill_2 }.each do |field| %>
   <td><%= style_info.send(field) -%></td>
  <% end %>
    <td><%= link_to 'Show', orgadmin_organization_style_info_path(@organization, style_info ) %></td>
    <td><%= link_to 'Edit', edit_orgadmin_organization_style_info_path(@organization, style_info) %></td>
    <td><%= link_to 'Destroy', orgadmin_organization_style_info_path(@organization, style_info), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New style_info', new_orgadmin_organization_style_info_path(@organization) %>
