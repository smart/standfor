<h1>Listing badges</h1>

<table>
  <tr>
  </tr>

<% for badge in @badges %>
  <tr>
    <td><%= link_to 'Show', admin_organization_badge_path(@organization, badge) %></td>
    <td><%= link_to 'Edit', edit_admin_organization_badge_path(@organization, badge) %></td>
    <td><%= link_to 'Destroy', admin_organization_badge_path(@organization, badge ), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New badge', new_admin_organization_badge_path(@organization) %>
