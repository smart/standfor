<h1>New configuration</h1>

<%= error_messages_for :configuration %>

<% form_for(@configuration, :url => admin_configuration_path(@configuration), :method => :post ) do |f| %>
  <ul>
    <%= render :partial => 'fields', :locals => {:f => f } %>
  <li>
    <%= f.submit "Create" %>
  </li>
 </ul>
<% end %>

<%= link_to 'Back', admin_configurations_path %>
