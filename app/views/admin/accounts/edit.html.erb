<h1>edit account attributes</h1>

<%= error_messages_for :account %>

<% form_for(@account,  :url => admin_account_path(@account) , :method => :put   )   do |f| %>
  <p>
   <label for="account_primary_email"> primary email</label>

    <%= f.text_field :primary_email -%> 
  </p>

  <p> 
   <%= f.submit "update account attributes" %>
  </p>

<% end %>

<p>
<%= link_to 'back to all accounts', admin_accounts_path %>
</p>
