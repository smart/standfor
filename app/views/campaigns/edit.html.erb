<h1>Editing campaign</h1>

<%= error_messages_for :campaign %>

<% form_for(@campaign, :url => organization_campaign_path(@organization), :method => :put ) do |f| %>

   <%= render :partial => 'fields', :locals => {:f => f, :organization => @organization} -%>

  <p>
    <%= f.submit "Update" %>
