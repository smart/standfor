<h1>Create a new campaign!</h1>

<%= error_messages_for :campaign %>
<% form_for(:campaign, :url =>  organization_campaigns_path(@organization), :method => :post ) do |f| %>
   <%= render :partial => 'fields', :locals => {:f => f, :organization => @organization} -%>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
<br />
 <%= link_to 'Back', organization_campaigns_path %>
