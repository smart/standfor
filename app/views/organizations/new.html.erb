<h1>New organization</h1>

<%= error_messages_for :organization %>

<% form_for(@organization) do |f| %>

  <p><lable for="organization_name" >Name:</label>
   <%= f.text_field :name-%>
  </p>

  <p><lable for="organization_address1" >Address Line One:</label>
   <%= f.text_field :address1-%>
  </p>

  <p><lable for="organization_address2" >Address Line Two:</label>
   <%= f.text_field :address2-%>
  </p>

  <p><lable for="organization_address3" >Address Line Three:</label>
   <%= f.text_field :address3-%>
  </p>

  <p><lable for="organization_city" >City:</label>
   <%= f.text_field :city-%>
  </p>

  <p><lable for="organization_state" >State:</label>
    <%= select 'organization', 'state', select_state_for_organization -%>
  </p>

  <p><lable for="organization_zip" >Zip:</label>
   <%= f.text_field :zip-%>
  </p>

  <p><lable for="organization_country" >Country:</label>
   <%= f.country_select :country, ['United States', 'Canada' ] -%>
  </p>

  <p><lable for="organization_phone" >Phone:</label>
   <%= f.text_field :phone-%>
  </p>

  <p><lable for="organization_fax" >Fax:</label>
   <%= f.text_field :fax-%>
  </p>

  <p><lable for="organization_email" >Email:</label>
   <%= f.text_field :email-%>
  </p>

  <p><lable for="organization_url" >Website:</label>
   <%= f.text_field :url-%>
  </p>

  <p><lable for="organization_statement" >Statement:</label>
   <%= f.text_area :statement-%>
  </p>

  <p><lable for="organization_description" >Description:</label>
   <%= f.text_area :description -%>
  </p>

  <p><lable for="organization_keyword" >Keyword:</label>
   <%= f.text_field :keyword-%>
  </p>

  <p><lable for="organization_site_name" >Site Name:</label>
   <%= f.text_field :site_name-%>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>

<% end %>

<%= link_to 'Back', organizations_path %>
