<% content_box( "sponsors", :class => "shadow" ) do -%>
  <% Sponsor.find(:all).each do |sponsor| %> 
   <% content_box( "#{sponsor.name}", :class => "shadow" ) do -%>
    <%= image_tag sponsor.sponsorslogo.public_filename(:medium) -%>
    <%= link_to sponsor.name,  sponsor_path(sponsor) -%>
   <% end %>
  <% end %>
<% end %>
